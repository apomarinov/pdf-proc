// left side
convert -crop 880x1345+105+178 3000*.jpg
// last left side
convert 3000ewords-92.jpg -crop 880x1345+105+178 3000ewords-92-91.jpg
// right side
convert -crop 885x1340+1170+185 3000*.jpg
// last right side
convert 3000ewords-92.jpg -crop 885x870+1170+185 3000ewords-92-91.jpg
// rename all with automator 1 - 184.jpg
// chop with php
for ($i=1; $i < 185; $i++) {
	exec("convert $i.jpg -chop 267x0+310+0 $i.jpg");
}
